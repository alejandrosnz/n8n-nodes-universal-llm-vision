{
  "name": "n8n-nodes-universal-llm-vision",
  "version": "0.2.1",
  "description": "n8n node for Universal LLM Vision",
  "keywords": [
    "n8n-community-node-package",
    "vision",
    "llm",
    "image-analysis"
  ],
  "license": "MIT",
  "homepage": "https://github.com/alejandrosnz/n8n-nodes-universal-llm-vision",
  "author": {
    "name": "Alejandro Sanz",
    "email": "alxbck1@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alejandrosnz/n8n-nodes-universal-llm-vision.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "type-check": "tsc --noEmit",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UniversalLlmVisionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/UniversalLlmVision/UniversalLlmVision.node.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@n8n/node-cli": "*",
    "@types/jest": "29.5.3",
    "babel-jest": "29.7.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "9.32.0",
    "jest": "29.7.0",
    "jest-mock-extended": "^4.0.0",
    "nock": "^14.0.10",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "ts-jest": "29.1.1",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
